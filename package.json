{
    "name": "npm-insights",
    "version": "1.0.6",
    "description": "Real time analytics of package downloads",
    "bin": {
        "npm-insights:init": "dist/init/index.js",
        "npm-insights:log": "dist/log/index.js"
    },
    "files": ["dist"],
    "watch": {
        "build": "{src,test}/*.js"
    },
    "scripts": {
        "flow": "flow",
        "watch": "npm-watch",
        "clean": "if [ -d ./dist ]; then rm -r ./dist; fi;mkdir ./dist;",
        "build-js": "babel -d dist/ src/",
        "build": "npm run clean; npm run build-js",
        "lint": "eslint src/",
        "test": "jest",
        "coverage": "jest --coverage",
        "precommit": "pretty-quick --staged",
        "prepare": "npm run build",
        "postinstall": "npm-insights:init"
    },
    "author": "Guillaume Morin",
    "license": "ISC",
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-eslint": "^8.2.3",
        "babel-jest": "^22.4.3",
        "babel-preset-env": "^1.7.0",
        "babel-preset-flow": "^6.23.0",
        "babel-preset-react": "^6.24.1",
        "eslint": "^4.19.1",
        "eslint-plugin-react": "^7.7.0",
        "eslint-plugin-react-native": "^3.2.1",
        "flow-bin": "^0.72.0",
        "husky": "^0.14.3",
        "jest": "^22.4.3",
        "npm-watch": "^0.3.0",
        "prettier": "1.12.1",
        "pretty-quick": "^1.4.1",
        "react-test-renderer": "^16.3.2",
        "regenerator-runtime": "^0.11.1"
    },
    "dependencies": {
        "apollo-cache-inmemory": "^1.2.1",
        "apollo-client": "^2.3.1",
        "apollo-link-http": "^1.5.4",
        "cli-color": "^1.2.0",
        "colorful-console-log": "^1.0.1",
        "graphql": "^0.13.2",
        "graphql-tag": "^2.9.2",
        "node-fetch": "^2.1.2",
        "print-message": "^2.1.0",
        "read-package-json": "^2.0.13",
        "systeminformation": "^3.41.3"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/guillaumemorin/npm-insights.git"
    },
    "bugs": {
        "url": "https://github.com/guillaumemorin/npm-insights/issues"
    },
    "homepage": "http://npm-insights.live",
    "keywords": ["npm", "insights", "downloads", "real-time", "analytics"]
}
